{
  "name": "final-task",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint src/**/*.ts",
    "format": "npx prettier --write .",
    "ci:format": "npx prettier --check .",
    "prepare": "cd .. && husky final-task/.husky",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "validate-branch-name": "^1.3.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "@commercetools/history-sdk": "^4.3.0",
    "@commercetools/importapi-sdk": "^5.4.0",
    "@commercetools/platform-sdk": "^7.8.0",
    "@commercetools/sdk-client-v2": "^2.5.0",
    "@commercetools/sdk-middleware-auth": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "navigo": "^8.11.1",
    "node-fetch": "^3.3.2",
    "sass": "^1.76.0",
    "vite": "^5.2.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "validate-branch-name": {
    "pattern": "^(chore|docs|feat|fix|refactor|style)/RSS-ECOMM-[0-9]_[a-zA-Z0-9-/]+$",
    "errorMsg": "Error: wrong name for branch!"
  }
}
